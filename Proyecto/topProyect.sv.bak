module topProyect (
	input clk,    // Clock
	input reset,  // Asynchronous reset active low
	input PS2_CLK, PS2_DATA,
	output hsync,
	output vsync,
	output wire [7:0] red,
	output wire [7:0] green,
	output wire [7:0] blue,
	output blank,
	output clkVGA
);
	logic KEY_COUNTER, swap;
	logic [31:0] ps2DataInput;
	logic [31:0] playerData, enemyData1, enemyData2;
	logic [7:0] LED;
	 
	initial begin
	
		LED <= 0;
		KEY_COUNTER <= 0;
		ps2DataInput <= 0;
		
	end

	Keyboard TestingKeyboard (.CLK(clk), .PS2_CLK(PS2_CLK), .PS2_DATA(PS2_DATA), .LED(LED), .LED2(ps2DataInput[1:0]), .KEY_COUNTER(KEY_COUNTER));
	
	top ArmProcesor(clk, ~reset, ps2DataInput, playerData, enemyData1, enemyData2);
	
	VGA VGA(clock,
				playerData[9:0],
				enemyData1[9:0],
				enemyData2[9:0],
				hsync,
				vsync,
				red,
				green,
				blue,
				blank,
				clkVGA);

endmodule